{
  "name": "yag-webapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "codequality:check": "npm run format:check && npm run lint:check && npm run types:check",
    "codequality:fix": "npm run format:fix && npm run lint:fix && npm run types:check",
    "dev": "next dev",
    "dev:all": "npm-run-all --parallel dev",
    "format:check": "prettier --check --ignore-unknown .",
    "format:fix": "prettier --write --ignore-unknown .",
    "lint:check": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix --max-warnings 0",
    "prepare": "husky",
    "start": "next start",
    "types:check": "tsc",
    "unused:check": "knip",
    "updates:check": "npm-check-updates -i"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.5",
    "@mui/material": "^5.16.5",
    "@mui/material-nextjs": "^5.16.4",
    "@next/third-parties": "^14.2.5",
    "@tanstack/react-query": "^5.51.15",
    "@types/lodash": "^4.17.7",
    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "next": "^14.2.32",
    "react": "^18",
    "react-dom": "^18",
    "react-infinite-scroll-hook": "^4.1.1",
    "tailwind-merge": "^2.4.0",
    "webrtc-adapter": "^9.0.1"
  },
  "devDependencies": {
    "@parcel/watcher": "^2.4.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-next": "^16.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "husky": "^9.1.3",
    "knip": "^5.27.0",
    "npm-check-updates": "^16.14.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-packagejson": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  }
}
